

<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge" />
	<meta http-equiv="X-UA-Compatible" content="ie=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title> Biniog - HYIP & Financial Investment Template </title>
	<!-- favicon -->
	<link rel="shortcut icon" href="assets/images/favicon.png" type="image/x-icon">
	<!-- bootstrap -->
	<link rel="stylesheet" href="assets/css/bootstrap.min.css">
	<!-- Plugin css -->
	<link rel="stylesheet" href="assets/css/plugin.css">
	<!-- Flaticon -->
	<link rel="stylesheet" href="assets/css/flaticon.css">

	<!-- stylesheet -->
	<link rel="stylesheet" href="assets/css/style.css">
	<link rel="stylesheet" href="assets/css/dark.css">
	<!-- responsive -->
	<link rel="stylesheet" href="assets/css/responsive.css">
</head>

<div class="mainmenu-area">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <nav class="navbar navbar-expand-lg navbar-light">
                    <a class="navbar-brand" href="{{ section.settings.link1 }}">{{ section.settings.link1text }}</a>
                    <button
                        class="navbar-toggler"
                        type="button"
                        data-toggle="collapse"
                        data-target="#main_menu"
                        aria-controls="main_menu"
                        aria-expanded="false"
                        aria-label="Toggle navigation"
                    >
                        {{ section.settings.button1 }}
                    </button>
                    <div class="collapse navbar-collapse fixed-height" id="main_menu">
                        <ul class="navbar-nav ml-auto">
                            <li class="nav-item">
                                <a class="nav-link active" href="{{ section.settings.link2 }}">
                                    {{- section.settings.link2text -}}
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="{{ section.settings.link3 }}">
                                    {{- section.settings.link3text -}}
                                </a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="{{ section.settings.link4 }}">
                                    {{- section.settings.link4text -}}
                                </a>
                            </li>
                        </ul>
                        <!-- <a href="signin.html" class="base-btn2"> Login</a> -->
                    </div>
                </nav>
            </div>
        </div>
    </div>
</div>
<!-- Main-Menu Area Start -->
{% schema %}
{
    "name": "header",
    "settings": [
        {
            "type": "image_picker",
            "id": "image1",
            "label": "Image 1"
        },
        {
            "type": "url",
            "id": "link1",
            "label": "Link 1"
        },
        {
            "type": "text",
            "id": "link1text",
            "label": "Link 1 text",
            "default": "{% if section.settings.image1 != blank %}{{ section.settings.image1 | image_url:  | image_tag: class: '', loading: 'lazy' }}{% endif %}"
        },
        {
            "type": "url",
            "id": "link2",
            "label": "Link 2"
        },
        {
            "type": "text",
            "id": "link2text",
            "label": "Link 2 text",
            "default": "Home"
        },
        {
            "type": "url",
            "id": "link3",
            "label": "Link 3"
        },
        {
            "type": "text",
            "id": "link3text",
            "label": "Link 3 text",
            "default": "About"
        },
        {
            "type": "url",
            "id": "link4",
            "label": "Link 4"
        },
        {
            "type": "text",
            "id": "link4text",
            "label": "Link 4 text",
            "default": "Contact"
        },
        {
            "type": "text",
            "id": "button1",
            "label": "Button 1"
        }
    ],
    "presets": [
        {
            "name": "header"
        }
    ]
}
{% endschema %}
